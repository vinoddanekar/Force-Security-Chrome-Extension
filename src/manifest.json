{
  "manifest_version": 2,
  "author": "Vinod Danekar",
  "name": "Force Security",
  "description": "Redirects all websites to https",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ]
    }
  ],
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage"
  ]
}